{
  "name": "productsAPI",
  "version": "1.0.0",
  "description": "API to fecth list of products",
  "main": "index.js",
  "repository": "https://github.com/folmosr/ProductsAPI",
  "author": "Fabian Olmos",
  "license": "MIT",
  "scripts": {
    "start:dev": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec ts-node-transpile-only ./src/index.ts",
    "start:dev:full": "concurrently -k -s first \"yarn run test:watch\" \"yarn run start:dev\"",
    "start": "node -r esm ./dist/src/index.js",
    "lint": "./node_modules/.bin/eslint \"src/**/*.{js,ts}\"",
    "lint:fix": "./node_modules/.bin/eslint \"src/**/*.{js,ts}\" --fix",
    "test": "jest -c jest.config.js --runInBand --detectOpenHandles --forceExit --bail",
    "test:coverage": "jest -c jest.config.js --coverage --runInBand --detectOpenHandles --forceExit --bail",
    "test:watch": "yarn run jest --watch",
    "test:integration": "./node_modules/.bin/jest --roots=./__tests__/integration",
    "prettier": "prettier --config .prettierrc \"src/**/*.ts\" --write"
  },
  "dependencies": {
    "@types/cors": "^2.8.17",
    "@types/joi": "^17.2.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "http-status": "^1.7.4",
    "joi": "^17.13.1",
    "mysql2": "^3.9.7",
    "sequelize": "^6.37.3",
    "uuidv4": "^6.2.13",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.5",
    "@babel/core": "^7.24.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-typescript": "^7.24.1",
    "@babel/register": "^7.23.7",
    "@babel/runtime": "^7.24.5",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.11",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "concurrently": "^8.2.2",
    "eslint": "^9.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "esm": "^3.2.25",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
